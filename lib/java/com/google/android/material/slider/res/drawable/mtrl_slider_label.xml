<rotate xmlns:android="http://schemas.android.com/apk/res/android"
    android:fromDegrees="45"
    android:toDegrees="45">
  <shape android:shape="rectangle">
    <solid android:color="@android:color/white" />
    <size
        android:width="@dimen/mtrl_slider_label_square_side"
        android:height="@dimen/mtrl_slider_label_square_side" />
    <corners
        android:bottomLeftRadius="@dimen/mtrl_slider_label_radius"
        android:topLeftRadius="@dimen/mtrl_slider_label_radius"
        android:topRightRadius="@dimen/mtrl_slider_label_radius" />
  </shape>
</rotate>
